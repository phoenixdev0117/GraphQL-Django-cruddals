{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Example Restaurant Project - Python Debugger Django",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/restaurantProject/manage.py",
      "args": [
        "runserver"
      ],
      "django": true,
      "justMyCode": false,
      "autoStartBrowser": false
    },
    {
      "name": "Run Test Project - Python Debugger Django",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test/cruddalsTestProject/manage.py",
      "args": [
        "runserver"
      ],
      "django": true,
      "autoStartBrowser": false
    },
    {
      "name": "Run Tests - Python Debugger Django",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test/cruddalsTestProject/manage.py",
      "args": [
        "test"
      ],
      "django": true,
      "autoStartBrowser": false
    },
    {
      "name": "Run Current File - Python Debugger",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}